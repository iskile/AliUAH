{
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Конвертер валют PrivatBank"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["*://*.aliexpress.com/*"],
      "run_at": "document_end"
    }
  ],
  "description": "Ali UAH. Конвертує ціну по курсу ПриватБанка на сайті AliExpress. By Winout96 and iSkile.",
  "icons": {
    "512": "icon.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "manifest_version": 3,
  "name": "Ali UAH",
  "permissions": ["tabs"],
  "host_permissions": ["*://*.aliexpress.com/"],
  "optional_host_permissions": ["*://*/*"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}
